
<body>
    <div>
        <div class="layui-anim layui-anim-up layui-anim-loop">
            <h2> 出租房列表</h2>
        </div>
        <hr class="layui-border-blue">
        <br />
        

        <div id="app" class="layui-anim layui-anim-rotate">           
            <table class="layui-table table table-bordered">
                <thead>
                    <tr class="layui-bg-orange">
                        <th>ID</th>
                        <th>小区名称</th>
                        <th>标题</th>
                        <th>面积</th>
                        <th>租金</th>
                        <th>排序</th>
                        <th>状态</th>
                    </tr>
                </thead>
                <tr v-for="item in emps">
                    <td v-text="item.id"></td>
                    <td v-text="item.name"></td>
                    <td v-text="item.title"></td>
                    <td v-text="item.proportion"></td>
                    <td v-text="item.charter"></td>
                    <td v-text="item.sort"></td>
                    <td v-text="item.state"></td>
                </tr>


            </table>

        </div>
    </div>



    <script type="text/javascript">
        new Vue({
            el: '#app',
            data: {
                emps: ''
            },
            mounted() {

                axios
                    .get('http://localhost:59475/api/Values/retal')
                    .then(response => (this.emps = response.data))
                    .catch(function (error) { // 请求失败处理
                        console.log(error);
                    });
            }
            


        })

    </script>

</body>
